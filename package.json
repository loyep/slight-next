{
  "name": "slight-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start:dev": "nodemon",
    "prebuild": "rimraf .next dist",
    "build": "next build && tsc --project tsconfig.server.json",
    "analyze": "cross-env BUNDLE_ANALYZE=true yarn build",
    "start": "next start",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "lint:js": "eslint --ext .ts,.tsx,.js --ignore-path .eslintignore .",
    "lint:style": "stylelint **/*.{less,scss,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "dependencies": {
    "@zeit/swr": "^0.1.2",
    "antd": "^4.7.0",
    "axios": "^0.20.0",
    "cross-env": "^7.0.2",
    "crypto": "^1.0.1",
    "dayjs": "^1.9.3",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "next": "9.5.5",
    "next-offline": "^5.0.2",
    "next-redux-wrapper": "^6.0.2",
    "next-routes": "^1.4.2",
    "nodemon": "^2.0.5",
    "nprogress": "^0.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-lazyload": "^3.0.0",
    "react-redux": "^7.2.1",
    "swiper": "^6.3.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.5.5",
    "@testing-library/react": "^11.1.0",
    "@types/classnames": "^2.2.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/less": "^3.0.1",
    "@types/next": "^9.0.0",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.52",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-jest": "^26.5.2",
    "babel-plugin-import": "^1.13.1",
    "css-loader": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.2.3",
    "jest": "^26.5.3",
    "jest-watch-typeahead": "^0.6.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.0.10",
    "next-compose-plugins": "^2.2.0",
    "null-loader": "^4.0.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
